node 
{                                              
def mvnhome
stage ("checkout code")
{
git 'https://github.com/awspriya/course.git'
mvnhome=tool 'testmvn'
}
stage ("compile")
{
sh "'${mvnhome}/bin/mvn' compile"
}
stage ("test")
{
sh "'${mvnhome}/bin/mvn' test"
}
stage ("Build")
{
sh "'${mvnhome}/bin/mvn' clean package"
cp -p /var/lib/jenkins/workspace/exapmplepipeline/target/course.war  /home/ec2-user/apache-tomcat-8.5.31/webapps/manager/
}
}





